{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Pet",
  "type": "object",
  "required": ["name", "photoUrls"],
  "properties": {
    "id": { "type": "number" },
    "category": {
      "type": "object",
      "required": [],
      "properties": {
        "id": { "type": "number" },
        "name": { "type": "string" }
      },
      "additionalProperties": true
    },
    "name": { "type": "string" },
    "photoUrls": {
      "type": "array",
      "items": { "type": "string" },
      "minItems": 1
    },
    "tags": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": { "type": "number" },
          "name": { "type": "string" }
        },
        "additionalProperties": true
      }
    },
    "status": { "type": "string", "enum": ["available", "pending", "sold"] }
  },
  "additionalProperties": true
}
